text--darken-3\">\r\n        Incomplete {{incomplete}}\r\n    </strong>\r\n    <v-spacer></v-spacer>\r\n       <strong class=\"info-text text--darken-3\">\r\n        Completed {{completed}}\r\n    </strong>\r\n    <v-spacer></v-spacer>\r\n        <v-progress-circular\r\n            :value=\"progress\"\r\n            class=\"mr-2\"\r\n          ></v-progress-circular>\r\n    </v-layout>\r\n    <v-card  v-if=\"todoList.length > 0\">\r\n        <v-slide-y-transition\r\n          class=\"py-0\"\r\n          group\r\n          tag=\"v-list\"\r\n        > \r\n         <template   \r\n         v-for=\"(item,index) in todoList\"\r\n        >\r\n        \r\n         <v-list-item\r\n         \r\n        :key=\"item.id\">\r\n         <v-list-item-action>\r\n              <v-checkbox \r\n              v-model=\"toggle[index]\"\r\n              on-icon=\"mdi-check\"\r\n              @click=\"complete(item.id)\"\r\n              ></v-checkbox>\r\n            </v-list-item-action>\r\n            <v-list-item-content>\r\n              <v-list-item-title><span :class=\"{completed:item.completed}\">{{item.text}}</span></v-list-item-title>\r\n              <v-list-item-subtitle>{{item.created_at}}</v-list-item-subtitle>\r\n            </v-list-item-content>\r\n            <v-list-item-icon>\r\n             <v-btn-toggle\r\n                v-show=\"toggle[index]\"\r\n                borderless\r\n                >\r\n              <v-btn icon small @click=\"deleteTodo(item.id,index)\">\r\n            <v-icon color=\"error\">mdi-delete</v-icon>\r\n          </v-btn>\r\n             </v-btn-toggle>\r\n            </v-list-item-icon>\r\n         </v-list-item>\r\n            <v-divider\r\n            v-if=\"index < todoList.length - 1\"\r\n            :key=\"index\"\r\n          ></v-divider>\r\n         </template>\r\n      </v-slide-y-transition> \r\n    </v-card> \r\n      <div v-else >\r\n         No item items found\r\n    </div>\r\n    </v-col>\r\n\r\n      </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport {mapGetters} from 'vuex';\r\nimport AddTodo from './AddTodo.vue';\r\n\r\nexport default {\r\n  data:() => ({\r\n     toggle:[],\r\n  }),\r\n  components: { AddTodo },\r\n  computed:{\r\n    ...mapGetters(['todoList']),\r\n    completed(){\r\n        return this.todoList.filter(todo => todo.completed).length\r\n    },\r\n        incomplete(){\r\n        return this.todoList.filter(todo => !todo.completed).length\r\n    },\r\n    progress(){\r\n        return this.completed/this.todoList.length * 100\r\n    },\r\n    color(){\r\n        return this.progress == 100 ? 'success' : 'info'\r\n    }\r\n  },\r\n  methods:{\r\n      deleteTodo(id,index){\r\n        this.$store.dispatch('deleteTodo', id)\r\n        .then(this.toggle[index] = false)\r\n      },\r\n      complete(id){\r\n          const item = this.todoList.find(todo=> todo.id == id);\r\n          item.completed = !item.completed;\r\n          this.$store.dispatch('updateTodo', item)\r\n      }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.completed{\r\n      text-decoration: line-through;\r\n}\r\n</style>"]}]}