WAAzB,qBAA+CxB,IAAI,CAACa,EAApD,GAAyDb,IAAzD,EACKyB,IADL,CACU;AAAA,eAAML,MAAM,CAAC,aAAD,EAAgBpB,IAAhB,CAAZ;AAAA,OADV,EAEK8B,KAFL,CAEW,UAACO,GAAD,EAAS;AACfN,QAAAA,OAAO,CAACC,GAAR,CAAYK,GAAZ;AACH,OAJF;AAKH;AAzBI;AArCgB,CAAf,CAAd;AAkEA,eAAe3C,KAAf","sourcesContent":["import Vue from 'vue'\r\nimport Vuex from 'vuex'\r\nimport api from './services/api'\r\n\r\nVue.use(Vuex)\r\n\r\nconst store = new Vuex.Store({\r\n    state: {\r\n        todos: [],\r\n        addTodo: {\r\n            visible: false,\r\n            todo: null,\r\n            mode: 'add'\r\n        }\r\n    },\r\n    getters: {\r\n        todoList: state => {\r\n            return state.todos\r\n        }\r\n    },\r\n    mutations: {\r\n        SHOW_DIALOG(state, payload) {\r\n            state.addTodo = {\r\n                visible: payload.visible,\r\n                mode: payload.mode,\r\n                todo: payload.todo\r\n            }\r\n        },\r\n        SET_TODOS(state, payload) {\r\n            state.todos = payload;\r\n        },\r\n        ADD_TODO(state, payload) {\r\n            state.todos.push(payload);\r\n        },\r\n        DELETE_TODO(state, payload) {\r\n            var item = state.todos.find(todo => todo.id == payload)\r\n            state.todos.splice(state.todos.indexOf(item), 1);\r\n        },\r\n        UPDATE_TODO(state, payload) {\r\n            var index = state.todos.indexOf(payload);\r\n            state.todos[index] = payload\r\n        }\r\n    },\r\n    actions: {\r\n        fetchTodos: async ({ commit }) => {\r\n            api().get(`${process.env.VUE_APP_URL}/all`)\r\n                .then((res) => commit('SET_TODOS', res.data))\r\n        },\r\n        addItem({ commit }, todo) {\r\n            api().post(`${process.env.VUE_APP_URL}/create`, todo)\r\n                .then((res) => commit('ADD_TODO', res.data))\r\n                .catch(() => {\r\n                    console.log(\"Could not add todo\")\r\n                })\r\n        },\r\n        deleteTodo({ commit }, id) {\r\n            api().delete(`${process.env.VUE_APP_URL}/delete/${id}`)\r\n                .then(() => commit('DELETE_TODO', id))\r\n                .catch(() => {\r\n                    console.log(\"Could not delete\")\r\n                })\r\n        },\r\n        updateTodo({ commit }, todo) {\r\n            api().put(`${process.env.VUE_APP_URL}/update/${todo.id}`,todo)\r\n                .then(() => commit('UPDATE_TODO', todo))\r\n                .catch((err) => {\r\n                 console.log(err)\r\n             })\r\n        }\r\n    }\r\n    })\r\n\r\nexport default store;"]}]}